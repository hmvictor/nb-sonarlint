<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN" "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>
    <folder name="Editors">
        <folder name="AnnotationTypes">
            <file name="sonarqube-info-annotation.xml" 
                  url="info-annotation.xml">
            </file>
            <file name="sonarqube-minor-annotation.xml" 
                  url="minor-annotation.xml">
            </file>
            <file name="sonarqube-major-annotation.xml" 
                  url="major-annotation.xml">
            </file>
            <file name="sonarqube-critical-annotation.xml" 
                  url="critical-annotation.xml">
            </file>
            <file name="sonarqube-blocker-annotation.xml" 
                  url="blocker-annotation.xml">
            </file>
        </folder>
    </folder>
    <folder name="TaskList">
        <folder name="Groups">
            <file name="radarlint.instance">
                <attr name="instanceCreate" methodvalue="org.netbeans.spi.tasklist.Task.createGroup"/>
                <attr name="localizingBundle" stringvalue="org.radar.radarlint.Bundle"/>
                <attr name="groupName" stringvalue="sonarlint-tasklist"/>
                <attr name="diplayNameKey" stringvalue="scanner.displayName"/>
                <attr name="descriptionKey" stringvalue="scanner.description"/>
                <attr name="iconKey" stringvalue="scanner.icon"/>
            </file>
        </folder>
<!--        <folder name="Scanners">
            <file name="SonarQubeTaskScanner.instance">
                <attr name="instanceOf" stringvalue="org.netbeans.spi.tasklist.PushTaskScanner"/>
                <attr name="instanceCreate" methodvalue="org.radar.radarlint.SonarQubeTaskScanner.create"/>
            </file>
        </folder>
        <folder name="Scanners">
            <file name="SonarQubeFileTaskScanner.instance">
                <attr name="instanceOf" stringvalue="org.netbeans.spi.tasklist.FileTaskScanner"/>
                <attr name="instanceCreate" methodvalue="org.radar.radarlint.SonarQubeFileTaskScanner.create"/>
            </file>
        </folder>-->
    </folder>
</filesystem>
